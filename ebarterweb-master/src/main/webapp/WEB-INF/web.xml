<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee webapp_4_0.xsd"
  version="4.0">
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
   <welcome-file>login.jsp</welcome-file>
</welcome-file-list>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.kgisl.ebarter.controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>fileimport</servlet-name>
    <servlet-class>com.kgisl.ebarter.controller.BrokerageController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>fileimport</servlet-name>
    <url-pattern>/fileimport</url-pattern>
  </servlet-mapping>
</web-app>